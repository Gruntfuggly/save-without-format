{
	"name": "save-without-format",
	"displayName": "Save Without Format",
	"description": "Save a file without auto formatting",
	"icon": "icon.png",
	"version": "0.0.1",
	"publisher": "Gruntfuggly",
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "save-without-format.save",
				"title": "Save Without Format"
			}
		],
		"keybindings": [
			{
				"command": "save-without-format.save",
				"key": "shift+alt+s",
				"when": "editorTextFocus && !editorReadonly"
			}
		]
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"eslint": "^3.6.0",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"publisherDisplayName": "Gruntfuggly"
	}
}
